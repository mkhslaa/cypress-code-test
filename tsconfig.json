{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonJS",
    "typeRoots": [
      "./typings",
      "../node_modules/@types"
    ],
    "types": [
      "cypress",
        "node"
    ]
  },
  "include": ["cypress/**/*.ts"],
  "exclude": [
    "node_modules"
  ]
}